Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"\<\"The logistic map is a polynomial mapping (equivalently, \
recurrence relation) of degree 2, often cited as an archetypal example of how \
complex, chaotic behaviour can arise from very simple non-linear dynamical \
equations.\"\>", 
   ",", "\<\"The map was popularized in a 1976 paper by the biologist Robert \
May, in part as a discrete-time demographic model analogous to the logistic \
equation first created by Pierre Fran\[CCedilla]ois Verhulst.\"\>"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.792705069993041*^9, 
  3.7927050983618097`*^9}},ExpressionUUID->"50b2bb4f-49cc-438f-9d79-\
d7824b9d68ba"]
}, Open  ]],

Cell["Logistic Map Formula:", "Text",
 CellChangeTimes->{{3.792705151972574*^9, 
  3.792705165579494*^9}},ExpressionUUID->"01f8d625-a5f8-4ac1-8da7-\
5c124da03246"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"def", " ", "logisticMap", 
    RowBox[{"(", 
     RowBox[{"x", " ", ",", "r"}], ")"}]}], ":", "\n", "    ", "cap"}], " ", 
  "=", " ", 
  RowBox[{"r", " ", "*", " ", 
   RowBox[{"(", 
    RowBox[{"x", " ", "-", " ", 
     RowBox[{"x", "**", "2"}]}], ")"}]}]}], "\n", 
 RowBox[{"    ", 
  RowBox[{"return", " ", "cap"}]}]}], "Code",
 CellChangeTimes->{
  3.792704533609742*^9},ExpressionUUID->"b164dac2-bd63-46d2-b9a1-\
65a5734dcb98"],

